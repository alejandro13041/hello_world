#include <string.h>
#include <stdio.h>
#include <stdlib.h>
#include <iostream>
#include <string>
#include <sstream>

using namespace std;

int ip_aleatory(int ip){

 
      volatile int n1[10];
      volatile int n2[10];
      volatile int n3[10];
      volatile int n4[10];
        


    for (int i = 0; i != ip; ++i)
    {

        n1[i] = 0 + rand() % (255); 
        n2[i] = 0 + rand() % (255); 
        n3[i] = 0 + rand() % (255); 
        n4[i] = 0 + rand() % (255); 
  
     }

         string IPs[ip];

  for (int i = 0; i < ip; ++i){


        IPs[i] = to_string(n1[i]) + '.' +
                 to_string(n2[i]) + '.' +
                 to_string(n3[i]) + '.' +
                 to_string(n4[i]);
   }

  cout<<"[server_$]:[cargando...]";
  cout<<"[server_$]:[complete aleatory_ip$]:";

  for (const auto &IP : IPs){
        
        cout << IP << '\n';
        
  }
  
}
int main (){


 cout<<"[server_$]:[ingrese cuantas ip4 va a generar $]:";
 int T;
 cin>>T;
 ip_aleatory(T);

	return 0;
}
